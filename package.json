{
  "name": "biostasis",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start-r": "react-native start --reset-cache",
    "test": "jest test",
    "test:watch": "jest test --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "android:staging": "react-native run-android --variant=stagingdebug",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:dev": "react-native run-android --variant=developmentdebug",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "ios:dev": "react-native run-ios --scheme 'BiostasisDevelopment'",
    "ios:staging": "react-native run-ios --scheme 'BiostasisStaging'",
    "ios:pod:install": "cd ios && pod install",
    "format": "prettier --write . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.0",

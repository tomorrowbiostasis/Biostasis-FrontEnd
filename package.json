    "dayjs": "^1.10.6",
    "formik": "^2.2.9",
    "i18next": "^20.3.2",
    "native-base": "3.0.3",
    "react": "17.0.2",
    "react-i18next": "^11.11.1",
    "react-native": "0.67.2",
    "react-native-background-fetch": "^4.0.3",
    "react-native-config": "^1.4.6",
    "react-native-debugger": "^1.1.0",
